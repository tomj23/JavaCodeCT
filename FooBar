	public static void main (String[] args) {
        printFooBar(100);	
	}
	
	public static void printFooBar(int number) {
	    
	    for(int i = 0; i <= number; i++) {
	        StringBuilder sb =  new StringBuilder("");
	        if(i%3 == 0) {
	            sb.append("Foo");
	        }
	        if(i%5 == 0) {
	            sb.append("Bar");
	        } 
	        System.out.println(i + " " + sb);
	    }   
	}
